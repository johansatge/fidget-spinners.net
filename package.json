{
  "name": "fidget-spinners.net",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "ejs": "^2.5.6",
    "filesize": "^3.5.6",
    "html-minifier": "^3.4.2",
    "node-sass": "^4.5.1",
    "osx-notifier": "^0.2.2",
    "uglifyjs-webpack-plugin": "^0.3.1",
    "webpack": "^2.3.2"
  },
  "scripts": {
    "watch": "chokidar --ignore=src/js/main.bundle.js src/**/* --command='npm run build'",
    "build": "rm -r dist;mkdir dist && cp -r src/images dist/images && cp -r spinners/images dist/images/spinners && node build/build.js && php build/build_versioned_assets.php;rm src/js/main.bundle.js",
    "deploy": "npm run build && rsync --archive --verbose --chmod=u=rwX,g=rX --exclude=.DS_Store dist/ $(head -n 1 .cdn)"
  },
  "engines": {
    "node": ">=6"
  }
}
