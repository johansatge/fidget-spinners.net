{
  "name": "fidget-spinners.net",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.0",
    "ejs": "^2.5.6",
    "filesize": "^3.5.6",
    "fs-extra": "^3.0.0",
    "html-minifier": "^3.4.2",
    "node-sass": "^4.5.1",
    "osx-notifier": "^0.2.2",
    "uglifyjs-webpack-plugin": "^0.3.1",
    "webpack": "^2.3.2"
  },
  "scripts": {
    "watch": "chokidar src/**/* --command='npm run build'",
    "build": "node build.js && php build_versioned_assets.php",
    "deploy": "npm run build && rsync --archive --checksum --verbose --chmod=u=rwX,g=rX --exclude=.DS_Store dist/ $(head -n 1 .cdn)"
  },
  "engines": {
    "node": ">=6"
  }
}
